{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning sass",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FariaParvinMegha/sass_learning_natours.git"
  },
  "author": "Megha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FariaParvinMegha/sass_learning_natours/issues"
  },
  "homepage": "https://github.com/FariaParvinMegha/sass_learning_natours#readme",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5"
  }
}
